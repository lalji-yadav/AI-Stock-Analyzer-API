{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/stock-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/stock-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/stock-api",
  "projects": {
    "stock-api": {
      "type": "application",
      "root": "apps/stock-api",
      "entryFile": "main",
      "sourceRoot": "apps/stock-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/stock-api/tsconfig.app.json"
      }
    },
    "check-stock": {
      "type": "application",
      "root": "apps/check-stock",
      "entryFile": "main",
      "sourceRoot": "apps/check-stock/src",
      "compilerOptions": {
        "tsConfigPath": "apps/check-stock/tsconfig.app.json"
      }
    }
  }
}